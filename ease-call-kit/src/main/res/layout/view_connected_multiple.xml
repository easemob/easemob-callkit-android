<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="54dp">

    <com.hyphenate.callkit.widget.CallKitImageView
        android:id="@+id/iv_connected_video_flip"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/callkit_button_on_background"
        android:padding="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/callkit_camera_back"
        app:layout_constraintEnd_toStartOf="@id/iv_connected_video_mic"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shape_type="round" />

    <TextView
        android:id="@+id/tv_connected_video_flip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/callkit_flip"
        android:textColor="@color/callkit_neutral_98"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/iv_connected_video_flip"
        app:layout_constraintStart_toStartOf="@id/iv_connected_video_flip"
        app:layout_constraintTop_toBottomOf="@id/iv_connected_video_flip" />

    <com.hyphenate.callkit.widget.CallKitImageView
        android:id="@+id/iv_connected_video_mic"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/callkit_button_on_background"
        android:padding="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/callkit_mic_on"
        app:layout_constraintEnd_toStartOf="@id/iv_connected_video_speaker"
        app:layout_constraintStart_toEndOf="@id/iv_connected_video_flip"
        app:layout_constraintTop_toTopOf="@id/iv_connected_video_flip"
        app:layout_constraintBottom_toBottomOf="@id/iv_connected_video_flip"
        app:shape_type="round" />

    <TextView
        android:id="@+id/tv_connected_video_mic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/callkit_mike_on"
        android:textColor="@color/callkit_neutral_98"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/iv_connected_video_mic"
        app:layout_constraintStart_toStartOf="@id/iv_connected_video_mic"
        app:layout_constraintTop_toBottomOf="@id/iv_connected_video_mic" />

    <com.hyphenate.callkit.widget.CallKitImageView
        android:id="@+id/iv_connected_video_speaker"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/callkit_button_on_background"
        android:padding="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/callkit_speaker_on"
        app:layout_constraintEnd_toStartOf="@id/iv_connected_video_camera"
        app:layout_constraintStart_toEndOf="@id/iv_connected_video_mic"
        app:layout_constraintTop_toTopOf="@id/iv_connected_video_flip"
        app:layout_constraintBottom_toBottomOf="@id/iv_connected_video_flip"
        app:shape_type="round" />

    <TextView
        android:id="@+id/tv_connected_video_speaker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/callkit_speaker_on"
        android:textColor="@color/callkit_neutral_98"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/iv_connected_video_speaker"
        app:layout_constraintStart_toStartOf="@id/iv_connected_video_speaker"
        app:layout_constraintTop_toBottomOf="@id/iv_connected_video_speaker" />

    <com.hyphenate.callkit.widget.CallKitImageView
        android:id="@+id/iv_connected_video_camera"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/callkit_button_on_background"
        android:padding="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/callkit_video_camera_on"
        app:layout_constraintEnd_toStartOf="@id/iv_connected_video_end"
        app:layout_constraintStart_toEndOf="@id/iv_connected_video_speaker"
        app:layout_constraintTop_toTopOf="@id/iv_connected_video_flip"
        app:layout_constraintBottom_toBottomOf="@id/iv_connected_video_flip"
        app:shape_type="round" />

    <TextView
        android:id="@+id/tv_connected_video_camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/callkit_camera_on"
        android:textColor="@color/callkit_neutral_98"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/iv_connected_video_camera"
        app:layout_constraintStart_toStartOf="@id/iv_connected_video_camera"
        app:layout_constraintTop_toBottomOf="@id/iv_connected_video_camera" />

    <ImageView
        android:id="@+id/iv_connected_video_end"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/callkit_end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/iv_connected_video_camera"
        app:layout_constraintTop_toTopOf="@id/iv_connected_video_flip"
        app:layout_constraintBottom_toBottomOf="@id/iv_connected_video_flip" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/callkit_end"
        android:textColor="@color/callkit_neutral_98"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/iv_connected_video_end"
        app:layout_constraintStart_toStartOf="@id/iv_connected_video_end"
        app:layout_constraintTop_toBottomOf="@id/iv_connected_video_end" />

</androidx.constraintlayout.widget.ConstraintLayout>